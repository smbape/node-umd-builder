deps = [
    {
        amd: 'lodash'   # in amd mode, lodash is available under lodash name
        common: '!_'    # in commonjs mode, lodash is available under global variable _
        node: 'lodash'  # in nodejs, lodash is available under lodash name
    }
    './sub-module'
]

factory = (_, sub)->
    library = _.extend {toto: 'tata'}, sub

    return library